# If you see this output, you probably forgot to pipe it into `source`:
# nix-your-shell | source /dev/stdin

function nix-shell () {
    {{ executable }} {{ extra_args | join(" ") }} {{ shell }} nix-shell -- "$@"
}

function nix () {
    {{ executable }} {{ extra_args | join(" ") }} {{ shell }} nix -- "$@"
}
